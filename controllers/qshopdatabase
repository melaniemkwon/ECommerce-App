create table itemInventory(
	id int auto_increment not null primary key ,
    itemName varchar(200),
    itemImage varchar(200),
    description varchar(1000),
    quantity int,
    itemPrice decimal(5,2)
);


create table cart(
	id int auto_increment not null primary key ,
    itemName varchar(200),
    itemImage varchar(200),
    itemQuantity int,
    itemPrice double
);

create table cart(
	id int auto_increment not null primary key ,
    items varchar(1000),
    price varchar(200),
);

